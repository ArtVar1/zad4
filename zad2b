package com.company;
import java.util.Scanner;
import static java.lang.Math.*;
public class Main {
    public static void main(String[] args) {
        int x, y,x2,y2;
        punkt(){
            this.x = 0;
            this.y = 0;
        }
        punkt(int x, int y){
            this.x = x;
            this.y = y;
        }
        void count_distance(int x, int y){
            System.out.println("odleg≈Çosc "+sqrt( (Math.pow(x+this.x, 2))+ (Math.pow(y+this.y, 2)));
        }
    }
